{
    "name": "CondenseAI",
    "version": "1.0.0",
    "description": "Get a summary of news articles in a click. AI article summaries for the curious mind",
    "manifest_version": 3,
    "author": "Wale Olowonyo F.",
    "action":{
        "default_popup": "index.html",
        "default_title": "CondenseAI Chrome Extension"
    },

    "permissions": [
        "tabs"
      ]
,
    "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
    }
    
}